<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.contract.ContractMapper">
  <select id="checkExists"  parameterType="org.system.entity.contract.Contract" resultType="map">
   select id,user_id,creator_id,doctor_group_id,state,source_id,first_send_sms_id,contract_number,contract_source,update_time  from contract 
   <where>
      <if test="creatorId!=null and creatorId!=''">
        creator_id=#{creatorId}
      </if>
       <if test="doctorGroupId!=null and doctorGroupId!=''">
        and  doctor_group_id=#{doctorGroupId}
      </if>
      and state=2
   </where>
  </select>
</mapper>

	@Override
	public Map<String, Object> getGxyFollowupDragRecordsDetail(GxyFollowupDragRecords gxyFollowupDragRecords) {
		return mapper.queryOne(gxyFollowupDragRecords);
	}

	@Override
	public int deleteGxyFollowupDragRecords(GxyFollowupDragRecords gxyFollowupDragRecords) {
		return mapper.delete(gxyFollowupDragRecords);
	}


	

	
	

package org.system.service.impl.service.visits;

import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.visits.GxyFollowupDragRecords;
import org.system.mapper.service.visits.GxyFollowupDragRecordsMapper;
import org.system.service.iface.service.visits.IGxyFollowupDragRecordsService;

@Service
public class GxyFollowupDragRecordsService implements IGxyFollowupDragRecordsService {

	@Resource
	private GxyFollowupDragRecordsMapper mapper;

	@Override
	public int insertGxyFollowupDragRecords(GxyFollowupDragRecords gxyFollowupDragRecords) {
		return mapper.insert(gxyFollowupDragRecords);
	}

	@Override
	public int updateGxyFollowupDragRecords(GxyFollowupDragRecords gxyFollowupDragRecords) {
		return mapper.update(gxyFollowupDragRecords);

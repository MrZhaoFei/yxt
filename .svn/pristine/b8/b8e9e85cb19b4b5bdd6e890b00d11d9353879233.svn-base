<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.doctor.SysDoctorLogsMapper" >

  <insert id="insert" parameterType="org.system.entity.doctor.SysDoctorLogs" >
    insert into sys_doctor_logs ( doctor_id, ip, 
      log_level, log_content, create_time
      )
    values ( #{doctorId}, #{ip}, 
      #{logLevel}, #{logContent}, #{createTime}
      )
  </insert>

</mapper>
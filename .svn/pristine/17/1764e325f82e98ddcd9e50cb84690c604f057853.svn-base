package org.system.service.impl;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.Address;
import org.system.mapper.service.AddressMapper;
import org.system.service.iface.IAddressService;


@Service
public class AddressService implements IAddressService{
	
	@Resource
	private AddressMapper addressMapper;

	@Override
	public int insertAddress(Address adress) {
		return addressMapper.insert(adress);
	}

	@Override
	public int updateAddress(Address adress) {
		return addressMapper.update(adress);
	}

	@Override
	public Map<String, Object> getAddressByFiled(Address address) {
		return addressMapper.checkExists(address);
	}

	@Override
	public List<Map<String, Object>> getAddressList(Address address) {
		return addressMapper.queryAll(address);
	}

	@Override
	public List<Map<String, Object>> getAddressChildList(Address address) {
		return addressMapper.getAddressChildList(address);
	}

	@Override
	public int deleteAddress(Address adress) {
		return addressMapper.delete(adress);
	}

	@Override
	public Map<String, Object> getAddressDetail(Address address) {
		return addressMapper.queryOne(address);
	}

	@Override
	public List<Map<String, Object>> getAddress(Address address) {
		return addressMapper.queryAddress(address);
	}

	@Override
	public Map<String, Object> getAddressMaxId(Address address) {
		
		return addressMapper.queryMaxId(address);
	}

	@Override
	public Map<String, Object> getAddressCodeValue(Address address) {
		return addressMapper.queryCodeValue(address);
	}

	
}

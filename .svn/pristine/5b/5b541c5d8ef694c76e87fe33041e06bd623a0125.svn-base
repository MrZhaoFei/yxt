package org.system.controller.iface.service.resp;

import java.util.Map;

import org.core.annotation.RequiresPermissions;
import org.springframework.validation.BindingResult;
import org.springframework.validation.annotation.Validated;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestHeader;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;
import org.springframework.web.bind.annotation.ResponseBody;
import org.system.entity.service.resp.FormNotExistDetaily;
import org.utils.spring.Groups;

/**
 * 无表单（流程）服务记录
 * 
 * @author ZF
 *
 */
@RequestMapping()
public interface IFormNotExistDetailyController {

	/**
	 * 医生新增 无表单（流程）服务记录
	 * 
	 * @param formNotExistDetaily
	 * @param resul
	 * @param token
	 * @return
	 */
	@RequestMapping(value = "/doctor/formNotExistDetaily", method = RequestMethod.POST)
	@ResponseBody
	@RequiresPermissions("formNotExistDetaily:insert:user")
	public Map<String, Object> insertFormNotExistDetaily(
			@Validated({ Groups.Insert.class }) FormNotExistDetaily formNotExistDetaily, BindingResult resul,
			@RequestHeader(value = "token", required = false) String token);

	/**
	 * 医生查询 无表单（流程）服务记录列表
	 * 
	 * @param transTreatDetaily
	 * @param result
	 * @param token
	 * @return
	 */
	@RequestMapping(value = "/doctor/formNotExistDetailys", method = RequestMethod.GET)
	@ResponseBody
	@RequiresPermissions("formNotExistDetaily:list")
	public Map<String, Object> getFormNotExistDetailyList(
			@Validated({ Groups.Select.class }) FormNotExistDetaily formNotExistDetaily, BindingResult result,
			@RequestHeader(value = "token", required = false) String token);

	/**
	 * 医生查询 无表单（流程）服务记录详情
	 * 
	 * @param serviceRespTaskId
	 * @param formNotExistDetaily
	 * @param result
	 * @param token
	 * @return
	 */
	@RequestMapping(value = { "/doctor/formNotExistDetaily/{taskId}" }, method = RequestMethod.GET)
	@ResponseBody
	@RequiresPermissions("formNotExistDetaily:detail")
	public Map<String, Object> getFormNotExistDetailyDetail(@PathVariable("taskId") Integer formNotExistDetailyId,
			FormNotExistDetaily formNotExistDetaily, BindingResult result,
			@RequestHeader(value = "token", required = false) String token);

	/**
	 * 用户查询 无表单（流程）服务记录详情
	 * 
	 * @param serviceRespTaskId
	 * @param formNotExistDetaily
	 * @param result
	 * @param token
	 * @return
	 */
	@RequestMapping(value = { "/user/formNotExistDetaily/{taskId}" }, method = RequestMethod.GET)
	@ResponseBody
	@RequiresPermissions("formNotExistDetaily:detail")
	public Map<String, Object> getFormNotExistDetailyDetailByUser(@PathVariable("taskId") Integer formNotExistDetailyId,
			FormNotExistDetaily formNotExistDetaily, BindingResult result,
			@RequestHeader(value = "token", required = false) String token);

}
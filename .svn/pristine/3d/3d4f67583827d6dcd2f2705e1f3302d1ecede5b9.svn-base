package org.system.service.impl.service;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.PackageType;
import org.system.mapper.service.PackageTypeMapper;
import org.system.service.iface.service.IPackageTypeService;

@Service
public class PackageTypeService implements IPackageTypeService {

	
	@Resource
	private PackageTypeMapper packageTypeMapper;
	@Override
	public List<Map<String, Object>> getPackageTypeList(PackageType packageType) {
		return packageTypeMapper.queryAll(packageType);
	}
	@Override
	public int updatePackageType(PackageType packageType) {
		return packageTypeMapper.update(packageType);
	}
	@Override
	public int deletePackageType(PackageType packageType) {
		return packageTypeMapper.delete(packageType);
	}
	@Override
	public Map<String, Object> getPackageTypeDetail(PackageType packageType) {
		return packageTypeMapper.queryOne(packageType);
	}
	@Override
	public Map<String, Object> getPackageTypeByFiled(PackageType packageType) {
		return packageTypeMapper.checkExists(packageType);
	}
	@Override
	public int insertPackageType(PackageType packageType) {
		return packageTypeMapper.insert(packageType);
	}

}

# 框架要点描述
# 框架采用Spring、SpringMVC、Mybatis搭建 按功划分模块
# 实现自动分页（MySql）
# 实现类似Shiro的权限管理（RBAC权限模型） 用户信息和权限缓存在Ehcache中
# 使用Hibernate Validator校验框架实现自动校验无需再方法内单个判断
# 实现Mybatis的逆向工程自动生成实体类
# 以云通讯作为短信接口
# 集成阿里云OSS Url签名和STS签名 以用于文件客户端直传

<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.user.DiseaseMapper" >
	<!-- 新增 -->
	<insert id="insert" parameterType="org.system.entity.Disease">
		
	</insert>
	
	<!-- 删除 -->
	<delete id="delete" parameterType="org.system.entity.Disease">
		
	</delete>
	
	<!-- 修改 -->
	<update id="update" parameterType="org.system.entity.Disease">
		
	</update>
	
	<!-- 查询列表 -->
	<select id="queryAll" parameterType="org.system.entity.Disease" resultType="map">
		
	</select>
	
	<!-- 根据主键查询单个 -->
	<select id="queryOne" parameterType="org.system.entity.Disease" resultType="map">
		
	</select>
	
	<!-- 根据约束键查询数据是否存在 -->
	<select id="checkExists" parameterType="org.system.entity.Disease" resultType="map">
	
	</select>
</mapper>

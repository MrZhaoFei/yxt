package org.system.service.impl.doctor;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.doctor.DoctorGroupType;
import org.system.mapper.doctor.DoctorGroupTypeMapper;
import org.system.service.iface.doctor.IDoctorGroupTypeService;
@Service
public class DoctorGroupTypeService  implements IDoctorGroupTypeService{
   
	@Resource
	private DoctorGroupTypeMapper  doctorGroupTypeMapper; 
	@Override
	public List<Map<String, Object>> getDoctorGroupTypeList(DoctorGroupType doctorGroupType) {
		
		return doctorGroupTypeMapper.queryAll(doctorGroupType);
	}

}

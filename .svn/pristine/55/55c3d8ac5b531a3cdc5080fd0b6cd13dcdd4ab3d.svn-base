package org.system.service.impl.service.transfer;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.transfer.YuyueCountInfo;
import org.system.mapper.service.transfer.YuyueCountInfoMapper;
import org.system.service.iface.service.transfer.IYuyueCountInfoService;

@Service
public class YuyueCountInfoService implements IYuyueCountInfoService {

	@Resource
	private YuyueCountInfoMapper mapper;

	@Override
	public Map<String, Object> getYuyueCountInfoDetail(YuyueCountInfo yuyueCountInfo) {
		return mapper.queryOne(yuyueCountInfo);
	}

	@Override
	public List<Map<String, Object>> getYuyueCountInfos(YuyueCountInfo yuyueCountInfo) {
		return mapper.queryAll(yuyueCountInfo);
	}

	@Override
	public int updateYuyueCountInfoCut(YuyueCountInfo yuyueCountInfo) {
		return mapper.update(yuyueCountInfo);
	}

}

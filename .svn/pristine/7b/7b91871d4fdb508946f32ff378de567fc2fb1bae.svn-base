package org.system.service.impl.user.healthinfo;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthinfo.AllergicHistory;
import org.system.mapper.user.healthinfo.AllergicHistoryMapper;
import org.system.service.iface.user.healthinfo.IAllergicHistoryService;
@Service
public class AllergicHistoryService implements IAllergicHistoryService{

	
	@Resource
	private AllergicHistoryMapper mapper;
	
	@Override
	public int insertAllergicHistory(AllergicHistory allergicHistory) {
		return mapper.insert(allergicHistory);
	}

	@Override
	public int updateAllergicHistory(AllergicHistory allergicHistory) {
		return mapper.update(allergicHistory);
	}

	@Override
	public Map<String, Object> getAllergicHistoryDetail(AllergicHistory allergicHistory) {
		return mapper.queryOne(allergicHistory);
	}

	@Override
	public List<Map<String, Object>> getAllergicHistorys(AllergicHistory allergicHistory) {
		return mapper.queryAll(allergicHistory);
	}

	@Override
	public int deleteAllergicHistory(AllergicHistory allergicHistory) {
		return mapper.delete(allergicHistory);
	}

}

package org.system.service.impl.service.resp;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.resp.ServiceRespProcessInfo;
import org.system.mapper.service.resp.ServiceRespProcessInfoMapper;
import org.system.service.iface.service.resp.IServiceRespProcessInfoService;

@Service
public class ServiceRespProcessInfoService implements IServiceRespProcessInfoService {
	@Resource
	private ServiceRespProcessInfoMapper mapper;

	@Override
	public Map<String, Object> getServiceRespProcessInfoByFild(ServiceRespProcessInfo serviceRespProcessInfo) {
		return mapper.checkExists(serviceRespProcessInfo);
	}

	@Override
	public int insertServiceRespProcessInfo(ServiceRespProcessInfo serviceRespProcessInfo) {
		return mapper.insert(serviceRespProcessInfo);
	}

	@Override
	public int updateServiceRespProcessInfo(ServiceRespProcessInfo serviceRespProcessInfo) {
		return mapper.update(serviceRespProcessInfo);
	}

	@Override
	public List<Map<String, Object>> getServiceRespProcessInfoList(ServiceRespProcessInfo serviceRespProcessInfo) {
		return mapper.queryAll(serviceRespProcessInfo);
	}

	@Override
	public Map<String, Object> getServiceRespProcessInfoDetail(ServiceRespProcessInfo serviceRespProcessInfo) {
		return mapper.queryOne(serviceRespProcessInfo);
	}
}

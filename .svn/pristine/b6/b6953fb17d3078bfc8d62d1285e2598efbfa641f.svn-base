package org.system.entity.service.resp;

import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.utils.spring.Groups;

public class FormNotExistDetaily extends BaseEntity {
	
    private Integer id;

    @NotNull(message = "{serviceRespTask.id.notnull.valid}", groups = { Groups.Insert.class })
    private Integer serviceRespTaskId;

    @NotNull(message = "{userService.id.notnull.valid}", groups = { Groups.Insert.class })
    private Integer userServiceId;

    private Date createTime;

    private String formDesc;

    private Integer fromDoctorId;

    @NotNull(message = "{formNotExistDetaily.status.notnull.valid}", groups = { Groups.Update.class })
    private Integer state;

    private Integer sourceTag;

    private String formNumber;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getServiceRespTaskId() {
        return serviceRespTaskId;
    }

    public void setServiceRespTaskId(Integer serviceRespTaskId) {
        this.serviceRespTaskId = serviceRespTaskId;
    }

    public Integer getUserServiceId() {
        return userServiceId;
    }

    public void setUserServiceId(Integer userServiceId) {
        this.userServiceId = userServiceId;
    }

    public Date getCreateTime() {
        return createTime;
    }

    public void setCreateTime(Date createTime) {
        this.createTime = createTime;
    }

    public String getFormDesc() {
        return formDesc;
    }

    public void setFormDesc(String formDesc) {
        this.formDesc = formDesc;
    }

    public Integer getFromDoctorId() {
        return fromDoctorId;
    }

    public void setFromDoctorId(Integer fromDoctorId) {
        this.fromDoctorId = fromDoctorId;
    }

    public Integer getState() {
        return state;
    }

    public void setState(Integer state) {
        this.state = state;
    }

    public Integer getSourceTag() {
        return sourceTag;
    }

    public void setSourceTag(Integer sourceTag) {
        this.sourceTag = sourceTag;
    }

    public String getFormNumber() {
        return formNumber;
    }

    public void setFormNumber(String formNumber) {
        this.formNumber = formNumber;
    }
    
	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class })
	public Integer getRows() {
		return super.getRows();
	}
    
}
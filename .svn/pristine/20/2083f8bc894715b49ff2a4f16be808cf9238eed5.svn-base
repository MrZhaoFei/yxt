package org.system.service.iface.doctor;

import java.util.List;
import java.util.Map;

import org.system.entity.doctor.DoctorGroup;

public interface IDoctorGroupService {
	/**
	 * 新增医生团队
	 * @param doctorGroup
	 * @return int
	 */
	    public int insertDoctorGroup(DoctorGroup doctorGroup);
		
	    /**
		 * 修改医生团队
		 * @param doctorGroup
		 * @return int
		 */
		public int updateDoctorGroup(DoctorGroup doctorGroup);
		
		/**
		 * 删除医生团队
		 * @param doctorGroup
		 * @return int
		 */
		public int deleteDoctorGroup(DoctorGroup doctorGroup);
		
		/**
		 * 所有医生团队列表
		 * @param doctorGroup
		 * @return  List<Map<String, Object>>
		 */
		public List<Map<String, Object>> getDoctorGroupList(DoctorGroup doctorGroup);
		
		/**
		 * 医生团队详细信息
		 * @param doctorGroup
		 * @return Map<String, Object>
		 */
		public Map<String, Object> getDoctorGroupDetail(DoctorGroup doctorGroup);
		
		/**
		 * 检查医生团队的唯一性
		 * @param doctorGroup
		 * @return Map<String, Object>
		 */
		public Map<String, Object> getDoctorGroupByFiled(DoctorGroup doctorGroup);
		
		/**
		 * 医生团队存在的医生
		 * @param doctorGroup
		 * @return List<Map<String, Object>>
		 */
        public List<Map<String, Object>> getDoctorGroupDoctorList(DoctorGroup doctorGroup);
        
        /**
    	 * 医生团队不存在的医生
    	 * @param doctorGroup
    	 * @return List<Map<String, Object>>
    	 */
        public List<Map<String, Object>> getDoctorGroupDoctorListNotIn(DoctorGroup doctorGroup);
}

package org.system.service.impl.user.healthtest;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthtest.UserDailyBloodPressure;
import org.system.mapper.user.healthtest.UserDailyBloodPressureMapper;
import org.system.service.iface.user.healthtest.IUserDailyBloodPressureService;

@Service
public class UserDailyBloodPressureService implements IUserDailyBloodPressureService {
	@Resource
	private UserDailyBloodPressureMapper mapper;

	@Override
	public Map<String, Object> getUserDailyBloodPressureByFild(UserDailyBloodPressure userDailyBloodPressure) {
		return mapper.checkExists(userDailyBloodPressure);
	}

	@Override
	public int insertUserDailyBloodPressure(UserDailyBloodPressure userDailyBloodPressure) {
		return mapper.insert(userDailyBloodPressure);
	}

	@Override
	public int updateUserDailyBloodPressure(UserDailyBloodPressure userDailyBloodPressure) {
		return mapper.update(userDailyBloodPressure);
	}

	@Override
	public List<Map<String, Object>> getUserDailyBloodPressureList(UserDailyBloodPressure userDailyBloodPressure) {
		return mapper.queryAll(userDailyBloodPressure);
	}

	@Override
	public Map<String, Object> getUserDailyBloodPressureDetail(UserDailyBloodPressure userDailyBloodPressure) {
		return mapper.queryOne(userDailyBloodPressure);
	}
}

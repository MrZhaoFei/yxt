package org.system.service.impl.user.healthtest;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthtest.UserDailyBloodSugar;
import org.system.mapper.user.healthtest.UserDailyBloodSugarMapper;
import org.system.service.iface.user.healthtest.IUserDailyBloodSugarService;

@Service
public class UserDailyBloodSugarService implements IUserDailyBloodSugarService {
	@Resource
	private UserDailyBloodSugarMapper mapper;

	@Override
	public Map<String, Object> getUserDailyBloodSugarByFild(UserDailyBloodSugar userDailyBloodSugar) {
		return mapper.checkExists(userDailyBloodSugar);
	}

	@Override
	public int insertUserDailyBloodSugar(UserDailyBloodSugar userDailyBloodSugar) {
		return mapper.insert(userDailyBloodSugar);
	}

	@Override
	public int updateUserDailyBloodSugar(UserDailyBloodSugar userDailyBloodSugar) {
		return mapper.update(userDailyBloodSugar);
	}

	@Override
	public List<Map<String, Object>> getUserDailyBloodSugarList(UserDailyBloodSugar userDailyBloodSugar) {
		return mapper.queryAll(userDailyBloodSugar);
	}

	@Override
	public Map<String, Object> getUserDailyBloodSugarDetail(UserDailyBloodSugar userDailyBloodSugar) {
		return mapper.queryOne(userDailyBloodSugar);
	}
}

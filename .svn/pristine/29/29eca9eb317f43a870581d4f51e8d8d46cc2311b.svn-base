package org.system.service.impl.sys;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.sys.SysCodeRecords;
import org.system.mapper.sys.SysCodeRecordsMapper;
import org.system.service.iface.sys.ISysCodeRecordsService;
@Service
public class SysCodeRecordsService implements ISysCodeRecordsService{

	@Resource
	private SysCodeRecordsMapper mapper;
	
	@Override
	public List<Map<String, Object>> getSysCodeRecordsList(SysCodeRecords sysCodeRecords) {
		return mapper.queryAll(sysCodeRecords);
	}

}

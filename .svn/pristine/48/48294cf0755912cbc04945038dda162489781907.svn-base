package org.system.service.impl.self;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.self.SelfTestAnswer;
import org.system.mapper.self.SelfTestAnswerMapper;
import org.system.service.iface.self.ISelfTestAnswerService;

@Service
public class SelfTestAnswerService implements ISelfTestAnswerService{

	@Resource
	private SelfTestAnswerMapper mapper;
	
	@Override
	public int insertSelfTestAnswer(SelfTestAnswer selfTestAnswer) {
		return mapper.insert(selfTestAnswer);
	}

	@Override
	public int deleteSelfTestAnswer(SelfTestAnswer selfTestAnswer) {
		return mapper.delete(selfTestAnswer);
	}

	@Override
	public int updateSelfTestAnswer(SelfTestAnswer selfTestAnswer) {
		return mapper.update(selfTestAnswer);
	}

	@Override
	public Map<String, Object> getSelfTestAnswerDetail(SelfTestAnswer selfTestAnswer) {
		return mapper.queryOne(selfTestAnswer);
	}

	@Override
	public List<Map<String, Object>> getSelfTestAnswers(SelfTestAnswer selfTestAnswer) {
		return mapper.queryAll(selfTestAnswer);
	}

	@Override
	public Map<String, Object> getSelfTestAnswerByFiled(SelfTestAnswer selfTestAnswer) {
		return mapper.checkExists(selfTestAnswer);
	}

}

package org.system.service.impl.doc;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.doc.DocTmplate;
import org.system.mapper.doc.DocTmplateMapper;
import org.system.service.iface.doc.IDocTmplateService;
@Service
public class DocTmplateService implements IDocTmplateService{
	
	@Resource
	private DocTmplateMapper mapper;

	@Override
	public int insertDocTmplate(DocTmplate docTmplate) {
		return mapper.insert(docTmplate);
	}

	@Override
	public int deleteDocTmplate(DocTmplate docTmplate) {
		return mapper.delete(docTmplate);
	}

	@Override
	public int updateDocTmplate(DocTmplate docTmplate) {
		return mapper.update(docTmplate);
	}

	@Override
	public Map<String, Object> getDocTmplateDetail(DocTmplate docTmplate) {
		return mapper.queryOne(docTmplate);
	}

	@Override
	public Map<String, Object> getDocTmplateByFiled(DocTmplate docTmplate) {
		return mapper.checkExists(docTmplate);
	}

	@Override
	public List<Map<String, Object>> getDocTmplates(DocTmplate docTmplate) {
		return mapper.queryAll(docTmplate);
	}

}

package org.system.service.impl.user.healthtest;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthtest.SelfTestResult;
import org.system.mapper.user.healthtest.SelfTestResultMapper;
import org.system.service.iface.user.healthtest.ISelfTestResultService;

@Service
public class SelfTestResultService implements ISelfTestResultService {
	@Resource
	private SelfTestResultMapper mapper;

	@Override
	public Map<String, Object> getSelfTestResultByFild(SelfTestResult selfTestResult) {
		return mapper.checkExists(selfTestResult);
	}

	@Override
	public int insertSelfTestResult(SelfTestResult selfTestResult) {
		return mapper.insert(selfTestResult);
	}

	@Override
	public int updateSelfTestResult(SelfTestResult selfTestResult) {
		return mapper.update(selfTestResult);
	}

	@Override
	public List<Map<String, Object>> getSelfTestResultList(SelfTestResult selfTestResult) {
		return mapper.queryAll(selfTestResult);
	}

	@Override
	public Map<String, Object> getSelfTestResultDetail(SelfTestResult selfTestResult) {
		return mapper.queryOne(selfTestResult);
	}
}

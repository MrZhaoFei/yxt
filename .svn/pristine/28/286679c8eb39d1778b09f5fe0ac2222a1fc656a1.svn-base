package org.system.controller.iface.service.transfer;

import java.util.Map;

import org.core.annotation.RequiresPermissions;
import org.springframework.validation.BindingResult;
import org.springframework.validation.annotation.Validated;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestHeader;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;
import org.springframework.web.bind.annotation.ResponseBody;
import org.system.entity.service.transfer.TransTreatPhoneRecordHistoryInfo;
import org.utils.spring.Groups;

@RequestMapping()
public interface ITransTreatPhoneRecordHistoryInfoController {

	/**
	 * 新增 转诊 电话历史记录表
	 * 
	 * @param transTreatPhoneRecordHistoryInfo
	 * @param result
	 * @param token
	 * @return
	 */
	@RequestMapping(value = "/doctor/transTreatPhoneRecordHistoryInfo", method = RequestMethod.POST)
	@ResponseBody
	@RequiresPermissions("transTreatPhoneRecordHistoryInfo:insert")
	public Map<String, Object> insertTransTreatPhoneRecordHistoryInfo(
			@Validated({ Groups.Insert.class }) TransTreatPhoneRecordHistoryInfo transTreatPhoneRecordHistoryInfo,
			BindingResult result, @RequestHeader(value = "token", required = false) String token);

	/**
	 * @Title: insertPhoneRecordHistoryInfo
	 * @Description: 医生新增通话记录
	 * @param transTreatPhoneRecordHistoryInfo
	 * @param result
	 * @param token
	 * @return Map<String,Object>
	 */
	@RequestMapping(value = "/doctor/phoneRecordHistoryInfo", method = RequestMethod.POST)
	@ResponseBody
	@RequiresPermissions("phoneRecordHistoryInfo:insert")
	public Map<String, Object> insertPhoneRecordHistoryInfo(
			@Validated({ Groups.Other.class }) TransTreatPhoneRecordHistoryInfo transTreatPhoneRecordHistoryInfo,
			BindingResult result, @RequestHeader(value = "token", required = false) String token);

	/**
	 * 结束电话历史记录表
	 * 
	 * @param transTreatPhoneRecordHistoryInfo
	 * @param result
	 * @param token
	 * @return
	 */
	@RequestMapping(value = "/doctor/endTransTreatPhoneRecord/{transPhoneRecordId}", method = RequestMethod.PUT)
	@ResponseBody
	@RequiresPermissions("transTreatPhoneRecordHistoryInfo:update:endPhone")
	public Map<String, Object> endTransTreatPhoneRecord(
			@PathVariable("transPhoneRecordId") Integer transTreatPhoneRecordHistoryInfoId,
			TransTreatPhoneRecordHistoryInfo transTreatPhoneRecordHistoryInfo, BindingResult result);

	/**
	 * @Title: getTransTreatPhoneRecordHistoryInfo
	 * @Description: 医生获取用户电话历史纪录
	 * @param transTreatPhoneRecordHistoryInfo
	 * @param result
	 * @param token
	 * @return Map<String,Object>
	 */
	@RequestMapping(value = "/doctor/transTreatPhoneRecordHistoryInfos", method = RequestMethod.GET)
	@ResponseBody
	@RequiresPermissions("transTreatPhoneRecordHistoryInfo:query")
	public Map<String, Object> getTransTreatPhoneRecordHistoryInfo(
			@Validated({ Groups.Select.class }) TransTreatPhoneRecordHistoryInfo transTreatPhoneRecordHistoryInfo,
			BindingResult result, @RequestHeader(value = "token", required = false) String token);
}
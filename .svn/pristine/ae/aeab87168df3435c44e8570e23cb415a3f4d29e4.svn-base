package org.system.service.impl.service;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.DoctorLevel;
import org.system.mapper.service.DoctorLevelMapper;
import org.system.service.iface.service.IDoctorLevelService;


@Service
public class DoctorLevelService implements IDoctorLevelService{
    
	@Resource
	private DoctorLevelMapper doctorLevelMapper;
	@Override
	public int insertDoctorLevel(DoctorLevel doctorLevel) {
		return doctorLevelMapper.insert(doctorLevel);
	}

	@Override
	public int updateDoctorLevel(DoctorLevel doctorLevel) {
		return doctorLevelMapper.update(doctorLevel);
	}

	@Override
	public int deleteDoctorLevel(DoctorLevel doctorLevel) {
		return doctorLevelMapper.delete(doctorLevel);
	}

	@Override
	public List<Map<String, Object>> getDoctorLevelList(DoctorLevel doctorLevel) {
		return doctorLevelMapper.queryAll(doctorLevel);
	}

	@Override
	public Map<String, Object> getDoctorLevelDetail(DoctorLevel doctorLevel) {
		return doctorLevelMapper.queryOne(doctorLevel);
	}

	@Override
	public Map<String, Object> getDoctorLevelByFiled(DoctorLevel doctorLevel) {
		return doctorLevelMapper.checkExists(doctorLevel);
	}

}

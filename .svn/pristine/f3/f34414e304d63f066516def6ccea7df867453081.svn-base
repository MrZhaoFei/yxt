package org.system.service.impl.sys;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.sys.SysHintMessage;
import org.system.mapper.sys.SysHintMessageMapper;
import org.system.service.iface.sys.ISysHintMessageService;
@Service
public class SysHintMessageService implements ISysHintMessageService{

	@Resource
	private SysHintMessageMapper mapper;
	
	
	@Override
	public int insertSysHintMessage(SysHintMessage sysHintMessage) {
		return mapper.insert(sysHintMessage);
	}

	@Override
	public int updateSysHintMessage(SysHintMessage sysHintMessage) {
		return mapper.update(sysHintMessage);
	}

	@Override
	public int deleteSysHintMessage(SysHintMessage sysHintMessage) {
		return mapper.delete(sysHintMessage);
	}

	@Override
	public Map<String, Object> getSysHintMessageByFiled(SysHintMessage sysHintMessage) {
		return mapper.checkExists(sysHintMessage);
	}

	@Override
	public Map<String, Object> getSysHintMessageDetail(SysHintMessage sysHintMessage) {
		return mapper.queryOne(sysHintMessage);
	}

	@Override
	public List<Map<String, Object>> getSysHintMessageList(SysHintMessage sysHintMessage) {
		return mapper.queryAll(sysHintMessage);
	}

	@Override
	public List<Map<String, Object>> getsysHintMessageMap(SysHintMessage sysHintMessage) {
		return mapper.getSysHintMessageMap(sysHintMessage);
	}

}

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.userService.UserServiceMapper" >

   <select id="queryAll" parameterType="org.system.entity.userService.UserService" resultType="map">
    select id,user_service_pack_id userServicePackId,product_service_id,times,lock_times,user_id  from user_service 
   <where>
   <if test="productServiceId!=null">
    product_service_id=#{productServiceId}
    </if>
    <if test="userServicePackId!=null">
    user_service_pack_id=#{userServicePackId}
    </if>
   </where> 
   </select>
</mapper>
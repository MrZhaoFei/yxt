package org.system.entity.service.transfer;

import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.utils.spring.Groups;

public class TransTreatPhoneRecordHistoryInfo extends BaseEntity {

	private Integer id;

	@NotNull(message = "{transTreatDetaily.id.notnull.valid}", groups = { Groups.Insert.class })
	private Integer transTreatDetailyId;

	private String from;

	// @NotNull(message = "{doctor.id.notnull.valid}", groups = {
	// Groups.Insert.class })
	private Integer fromDoctorId;

	private String to;
	private String recordLog;

	@NotNull(message = "{user.id.notnull.valid}", groups = { Groups.Select.class, Groups.Other.class })
	private Integer toUserId;

	private Date startTime;

	private Date endTime;

	private String recordUrl;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getTransTreatDetailyId() {
		return transTreatDetailyId;
	}

	public void setTransTreatDetailyId(Integer transTreatDetailyId) {
		this.transTreatDetailyId = transTreatDetailyId;
	}

	public String getFrom() {
		return from;
	}

	public void setFrom(String from) {
		this.from = from;
	}

	public Integer getFromDoctorId() {
		return fromDoctorId;
	}

	public String getRecordLog() {
		return recordLog;
	}

	public void setRecordLog(String recordLog) {
		this.recordLog = recordLog;
	}

	public void setFromDoctorId(Integer fromDoctorId) {
		this.fromDoctorId = fromDoctorId;
	}

	public String getTo() {
		return to;
	}

	public void setTo(String to) {
		this.to = to;
	}

	public Integer getToUserId() {
		return toUserId;
	}

	public void setToUserId(Integer toUserId) {
		this.toUserId = toUserId;
	}

	public Date getStartTime() {
		return startTime;
	}

	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}

	public Date getEndTime() {
		return endTime;
	}

	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}

	public String getRecordUrl() {
		return recordUrl;
	}

	public void setRecordUrl(String recordUrl) {
		this.recordUrl = recordUrl;
	}

	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class })
	public Integer getRows() {
		return super.getRows();
	}
}
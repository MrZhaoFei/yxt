package org.system.service.impl.service;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.ServicePack;
import org.system.mapper.service.ServicePackMapper;
import org.system.service.iface.service.IServicePackService;

@Service
public class ServicePackService implements IServicePackService {
	@Resource
	private ServicePackMapper mapper;

	@Override
	public Map<String, Object> getServicePackByFild(ServicePack servicePack) {
		return mapper.checkExists(servicePack);
	}

	@Override
	public int insertServicePack(ServicePack servicePack) {
		return mapper.insert(servicePack);
	}

	@Override
	public int updateServicePack(ServicePack servicePack) {
		return mapper.update(servicePack);
	}

	@Override
	public List<Map<String, Object>> getServicePackList(ServicePack servicePack) {
		return mapper.queryAll(servicePack);
	}

	@Override
	public Map<String, Object> getServicePackDetail(ServicePack servicePack) {
		return mapper.queryOne(servicePack);
	}

	@Override
	public List<Map<String, Object>> getServicePackListByDoctor(ServicePack servicePack) {
		return mapper.getServicePackListByDoctor(servicePack);
	}

	@Override
	public List<Map<String, Object>>  getServicePackDetailByBase(ServicePack servicePack) {
		return mapper.getServicePackDetailByBase(servicePack);
	}
}

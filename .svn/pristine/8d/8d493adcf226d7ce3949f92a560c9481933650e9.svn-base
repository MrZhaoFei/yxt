package org.system.service.impl.order;



import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.order.PayInfo;
import org.system.mapper.order.PackagePayInfoMapper;
import org.system.service.iface.order.IPackagePayInfoService;


@Service
public class PackagePayInfoService implements IPackagePayInfoService{

	@Resource
	private PackagePayInfoMapper mapper;

	@Override
	public int insertPayinfo(PayInfo payinfo) {
		return mapper.insert(payinfo);
	}
}

package org.system.entity.sys;

import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.utils.spring.Groups;

public class SysAbout extends BaseEntity {
	private Integer id;

	private String title;

	private String covers;

	private String files;

	private Date createDate;

	private String type;

	private String content;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getCovers() {
		return covers;
	}

	public void setCovers(String covers) {
		this.covers = covers;
	}

	public String getFiles() {
		return files;
	}

	public void setFiles(String files) {
		this.files = files;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class })
	public Integer getRows() {
		return super.getRows();
	}
}
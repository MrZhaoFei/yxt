<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.userService.UserServicePackMapper" >
   <select id="queryAll" parameterType="org.system.entity.userService.UserServicePack" resultType="map">
      select id,user_id,service_pack_id ,create_time,price,state,expire_time from user_service_pack where service_pack_id=#{servicePackId}
   </select>
</mapper>
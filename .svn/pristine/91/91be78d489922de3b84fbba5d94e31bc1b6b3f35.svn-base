<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.user.healthtest.SelfTestQuestionMapper" >
	<!-- 新增 -->
	<insert id="insert" parameterType="org.system.entity.user.healthtest.SelfTestQuestion">
		
	</insert>
	
	<!-- 删除 -->
	<delete id="delete" parameterType="org.system.entity.user.healthtest.SelfTestQuestion">
		
	</delete>
	
	<!-- 修改 -->
	<update id="update" parameterType="org.system.entity.user.healthtest.SelfTestQuestion">
		
	</update>
	
	<resultMap type="map" id="questionMap">
		 <result property="id" column="id" />
		<collection property="answer" column="id" javaType="list" select="getAnswer"/>
	</resultMap>
	<!-- 查询列表 -->
	<select id="queryAll" parameterType="org.system.entity.user.healthtest.SelfTestQuestion" resultMap="questionMap">

	</select>
	
	<!-- 根据主键查询单个 -->
	<select id="queryOne" parameterType="org.system.entity.user.healthtest.SelfTestQuestion" resultType="map">
		
	</select>
	
	<!-- 根据约束键查询数据是否存在 -->
	<select id="checkExists" parameterType="org.system.entity.user.healthtest.SelfTestQuestion" resultType="map">
	
	</select>
</mapper>

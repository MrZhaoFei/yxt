package org.system.service.impl.user.healthinfo;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthinfo.HealthInfo;
import org.system.mapper.user.healthinfo.HealthInfoMapper;
import org.system.service.iface.user.healthinfo.IHealthInfoService;

@Service
public class HealthInfoService implements IHealthInfoService {

	@Resource
	private HealthInfoMapper mapper;

	@Override
	public int insertHealthInfo(HealthInfo healthInfo) {
		return mapper.insert(healthInfo);
	}

	@Override
	public int updatetHealthInfo(HealthInfo healthInfo) {
		return mapper.update(healthInfo);
	}

	@Override
	public List<Map<String, Object>> getHealthInfoList(HealthInfo healthInfo) {
		return mapper.queryAll(healthInfo);
	}

	@Override
	public Map<String, Object> getHealthInfoDetail(HealthInfo healthInfo) {
		return mapper.queryOne(healthInfo);
	}

}

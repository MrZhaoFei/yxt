package org.system.entity.doctor;

import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.hibernate.validator.constraints.NotBlank;
import org.springframework.format.annotation.DateTimeFormat;
import org.system.entity.user.User;
import org.system.entity.user.UserDetail;
import org.utils.spring.Groups;

public class Doctor extends BaseEntity {
	public interface WechatBind {
	};
	public interface UpdatePhone {
	}

	public interface UpdatePassword {
	}

	public interface RetrievePassword {
	}

	public interface getDoctorList {
	}

	private Integer id;

	private Integer registerSourceId;

	/**
	 * @fieldName: userServiceId
	 * @fieldType: Integer
	 * @Description: 用于根据服务筛选医生
	 */
	@NotNull(message = "{userService.id.notnull.valid}", groups = { getDoctorList.class })
	private Integer userServiceId;
	private Integer hospitalId;
	private Integer departmentId;

	private Integer doctorTypeId;

	private Integer doctorTypeOrderLevel;

	private Integer doctorLevelOrderLevel;

	private Integer addressId;

	private Integer titleId;

	private Boolean isValid;
	@NotBlank(message = "{doctor.name.notblank.valid}", groups = { Groups.Select.class, Groups.Insert.class,
			RetrievePassword.class })
	private String name;

	@NotBlank(message = "{phone.notblank.valid}", groups = { UpdatePhone.class,WechatBind.class })
	private String phone;
	@NotBlank(message = "{oldPassword.notblank.valid}", groups = { UpdatePassword.class })
	private String oldPassword;
	@NotBlank(message = "{password.notblank.valid}", groups = { UpdatePassword.class, Groups.Select.class,
			RetrievePassword.class, Groups.Insert.class })
	private String password;

	@NotBlank(message = "{wechatId.notblank.valid}", groups = { WechatBind.class })
	private String wechatId;

	@NotBlank(message = "{unionId.notblank.valid}", groups = { WechatBind.class })
	private String unionId;

	private Integer state;

	private Date registerDate;

	private String remark;
	
	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date startDate;
	
	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date endDate;
	
	private User user;
	
	private UserDetail userDetail;
	
	
	public String getUnionId() {
		return unionId;
	}

	public void setUnionId(String unionId) {
		this.unionId = unionId;
	}

	public Date getStartDate() {
		return startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public UserDetail getUserDetail() {
		return userDetail;
	}

	public void setUserDetail(UserDetail userDetail) {
		this.userDetail = userDetail;
	}

	public Integer getDoctorLevelOrderLevel() {
		return doctorLevelOrderLevel;
	}

	public void setDoctorLevelOrderLevel(Integer doctorLevelOrderLevel) {
		this.doctorLevelOrderLevel = doctorLevelOrderLevel;
	}

	public String getOldPassword() {
		return oldPassword;
	}

	public void setOldPassword(String oldPassword) {
		this.oldPassword = oldPassword;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getRegisterSourceId() {
		return registerSourceId;
	}

	public void setRegisterSourceId(Integer registerSourceId) {
		this.registerSourceId = registerSourceId;
	}

	public Integer getDoctorTypeId() {
		return doctorTypeId;
	}

	public Integer getHospitalId() {
		return hospitalId;
	}

	public void setHospitalId(Integer hospitalId) {
		this.hospitalId = hospitalId;
	}

	public Integer getDepartmentId() {
		return departmentId;
	}

	public void setDepartmentId(Integer departmentId) {
		this.departmentId = departmentId;
	}

	public void setDoctorTypeId(Integer doctorTypeId) {
		this.doctorTypeId = doctorTypeId;
	}

	public Integer getAddressId() {
		return addressId;
	}

	public Integer getUserServiceId() {
		return userServiceId;
	}

	public void setUserServiceId(Integer userServiceId) {
		this.userServiceId = userServiceId;
	}

	public void setAddressId(Integer addressId) {
		this.addressId = addressId;
	}

	public Integer getTitleId() {
		return titleId;
	}

	public void setTitleId(Integer titleId) {
		this.titleId = titleId;
	}

	public Boolean getIsValid() {
		return isValid;
	}

	public void setIsValid(Boolean isValid) {
		this.isValid = isValid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getWechatId() {
		return wechatId;
	}

	public void setWechatId(String wechatId) {
		this.wechatId = wechatId;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public Date getRegisterDate() {
		return registerDate;
	}

	public void setRegisterDate(Date registerDate) {
		this.registerDate = registerDate;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Integer getDoctorTypeOrderLevel() {
		return doctorTypeOrderLevel;
	}

	public void setDoctorTypeOrderLevel(Integer doctorTypeOrderLevel) {
		this.doctorTypeOrderLevel = doctorTypeOrderLevel;
	}

	@Override
	@NotNull(message = "{page.empty}", groups = { getDoctorList.class, Groups.Other.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { getDoctorList.class, Groups.Other.class })
	public Integer getRows() {
		return super.getRows();
	}

}
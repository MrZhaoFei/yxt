package org.system.service.impl.content;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.content.Content;
import org.system.mapper.content.ContentMapper;
import org.system.service.iface.content.IContentService;

@Service
public class ContentService implements IContentService{

	@Resource
	private ContentMapper mapper;

	@Override
	public List<Map<String, Object>> getContents(Content content) {
		return mapper.queryAll(content);
	}
}

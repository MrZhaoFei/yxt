package org.system.service.impl.doctor;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.doctor.DoctorGroupRely;
import org.system.mapper.doctor.DoctorGroupRelyMapper;
import org.system.service.iface.doctor.IDoctorGroupRelyService;
@Service
public class DoctorGroupRelyService implements IDoctorGroupRelyService{

	
	@Resource
	private DoctorGroupRelyMapper mapper;
	
	@Override
	public int insertDoctorGroupRely(DoctorGroupRely doctorGroupRely) {
		return mapper.insert(doctorGroupRely);
	}

	@Override
	public int updateDoctorGroupRely(DoctorGroupRely doctorGroupRely) {
		return mapper.update(doctorGroupRely);
	}

	@Override
	public Map<String, Object> getDoctorGroupRelyByFiled(DoctorGroupRely doctorGroupRely) {
		return mapper.checkExists(doctorGroupRely);
	}

	@Override
	public Map<String, Object> getDoctorGroupRelyDetail(DoctorGroupRely doctorGroupRely) {
		return mapper.queryOne(doctorGroupRely);
	}

	@Override
	public List<Map<String, Object>> getDoctorGroupRelyList(DoctorGroupRely doctorGroupRely) {
		return mapper.queryAll(doctorGroupRely);
	}

	@Override
	public int deleteDoctorGroupRely(DoctorGroupRely doctorGroupRely) {
		return mapper.delete(doctorGroupRely);
	}

	


	
}

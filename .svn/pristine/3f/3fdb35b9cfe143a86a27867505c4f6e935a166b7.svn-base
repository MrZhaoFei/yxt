package org.system.service.impl.service.visits;

import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.visits.TnbFollowupDragRecords;
import org.system.mapper.service.visits.TnbFollowupDragRecordsMapper;
import org.system.service.iface.service.visits.ITnbFollowupDragRecordsService;

@Service
public class TnbFollowupDragRecordsService implements ITnbFollowupDragRecordsService {

	@Resource
	private TnbFollowupDragRecordsMapper mapper;

	@Override
	public int insertTnbFollowupDragRecords(TnbFollowupDragRecords tnbFollowupDragRecords) {
		return mapper.insert(tnbFollowupDragRecords);
	}

	@Override
	public int updateTnbFollowupDragRecords(TnbFollowupDragRecords tnbFollowupDragRecords) {
		return mapper.update(tnbFollowupDragRecords);
	}

	@Override
	public Map<String, Object> getTnbFollowupDragRecordsDetail(TnbFollowupDragRecords tnbFollowupDragRecords) {
		return mapper.queryOne(tnbFollowupDragRecords);
	}

	@Override
	public int deleteTnbFollowupDragRecords(TnbFollowupDragRecords tnbFollowupDragRecords) {
		return mapper.delete(tnbFollowupDragRecords);
	}

	
}

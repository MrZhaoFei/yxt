package org.system.service.impl.about;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.about.SysAbout;
import org.system.mapper.about.SysAboutMapper;
import org.system.service.iface.about.ISysAboutService;

@Service
public class SysAboutService implements ISysAboutService {
	
	@Resource
	private SysAboutMapper sysAboutMapper;

	@Override
	public int insertSysAbout(SysAbout sysAbout) {
		return sysAboutMapper.insert(sysAbout);
	}

	@Override
	public int deleteSysAbout(SysAbout sysAbout) {
		return sysAboutMapper.delete(sysAbout);
	}

	@Override
	public int updateSysAbout(SysAbout sysAbout) {
		return sysAboutMapper.update(sysAbout);
	}

	@Override
	public Map<String, Object> getSysAboutDetail(SysAbout sysAbout) {
		return sysAboutMapper.queryOne(sysAbout);
	}

	@Override
	public Map<String, Object> getSysAboutByFiled(SysAbout sysAbout) {
		return sysAboutMapper.checkExists(sysAbout);
	}

	@Override
	public List<Map<String, Object>> getSysAbout(SysAbout sysAbout) {
		return sysAboutMapper.queryAll(sysAbout);
	}

}

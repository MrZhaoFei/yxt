package org.system.service.impl.service.resp;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.resp.TransRecordsInfo;
import org.system.mapper.service.resp.TransRecordsInfoMapper;
import org.system.service.iface.service.resp.ITransRecordsInfoService;

@Service
public class TransRecordsInfoService implements ITransRecordsInfoService {
	@Resource
	private TransRecordsInfoMapper mapper;

	@Override
	public Map<String, Object> getTransRecordsInfoByFild(TransRecordsInfo transRecordsInfo) {
		return mapper.checkExists(transRecordsInfo);
	}

	@Override
	public int insertTransRecordsInfo(TransRecordsInfo transRecordsInfo) {
		return mapper.insert(transRecordsInfo);
	}

	@Override
	public int updateTransRecordsInfo(TransRecordsInfo transRecordsInfo) {
		return mapper.update(transRecordsInfo);
	}

	@Override
	public List<Map<String, Object>> getTransRecordsInfoList(TransRecordsInfo transRecordsInfo) {
		return mapper.queryAll(transRecordsInfo);
	}

	@Override
	public Map<String, Object> getTransRecordsInfoDetail(TransRecordsInfo transRecordsInfo) {
		return mapper.queryOne(transRecordsInfo);
	}
}

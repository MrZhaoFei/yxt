package org.system.service.impl.userService;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.userService.UserServicePack;
import org.system.mapper.userService.UserServicePackMapper;
import org.system.service.iface.userService.IUserServicePackService;
@Service
public class UserServicePackService implements IUserServicePackService{
	
	
	
	
	@Resource
	private UserServicePackMapper userServicePackMapper;

	@Override
	public List<Map<String, Object>> getUserServicePackList(UserServicePack userServicePack) {
		return userServicePackMapper.queryAll(userServicePack);
	}

}

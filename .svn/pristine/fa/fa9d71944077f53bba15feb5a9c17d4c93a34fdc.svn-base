package org.system.entity.service;

import java.math.BigDecimal;
import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.utils.spring.Groups;

public class UserServicePack extends BaseEntity {

	private Integer id;

	@NotNull(message = "{user.id.notnull.valid}", groups = { Groups.Insert.class, Groups.Other.class })
	private Integer userId;

	@NotNull(message = "{servicePack.id.notnull.valid}", groups = { Groups.Insert.class })
	private Integer servicePackId;

	private Integer addressId;

	private Integer state;
	private BigDecimal price;
	private Date expireTime;

	private Date createTime;

	private Integer packageSourceId;
	/* 下列属性用于医生线下购买记录 */
	private Integer doctorId;
	private Integer hospitalId;
	private String evidenceSn;
	private String evidenceObjectId;
	private Date orderDate;

	/* 下列属性用于查询用户服务包 */
	private ServicePack servicePack;

	public ServicePack getServicePack() {
		return servicePack;
	}

	public void setServicePack(ServicePack servicePack) {
		this.servicePack = servicePack;
	}

	public Integer getDoctorId() {
		return doctorId;
	}

	public void setDoctorId(Integer doctorId) {
		this.doctorId = doctorId;
	}

	public Integer getHospitalId() {
		return hospitalId;
	}

	public void setHospitalId(Integer hospitalId) {
		this.hospitalId = hospitalId;
	}

	public String getEvidenceSn() {
		return evidenceSn;
	}

	public void setEvidenceSn(String evidenceSn) {
		this.evidenceSn = evidenceSn;
	}

	public String getEvidenceObjectId() {
		return evidenceObjectId;
	}

	public void setEvidenceObjectId(String evidenceObjectId) {
		this.evidenceObjectId = evidenceObjectId;
	}

	public Date getOrderDate() {
		return orderDate;
	}

	public void setOrderDate(Date orderDate) {
		this.orderDate = orderDate;
	}

	public Integer getPackageSourceId() {
		return packageSourceId;
	}

	public void setPackageSourceId(Integer packageSourceId) {
		this.packageSourceId = packageSourceId;
	}

	public Integer getAddressId() {
		return addressId;
	}

	public void setAddressId(Integer addressId) {
		this.addressId = addressId;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Integer getServicePackId() {
		return servicePackId;
	}

	public void setServicePackId(Integer servicePackId) {
		this.servicePackId = servicePackId;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public BigDecimal getPrice() {
		return price;
	}

	public void setPrice(BigDecimal price) {
		this.price = price;
	}

	public Date getExpireTime() {
		return expireTime;
	}

	public void setExpireTime(Date expireTime) {
		this.expireTime = expireTime;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class, Groups.Other.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class, Groups.Other.class })
	public Integer getRows() {
		return super.getRows();
	}
}
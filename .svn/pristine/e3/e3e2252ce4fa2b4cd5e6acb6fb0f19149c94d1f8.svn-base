package org.system.service.impl.user;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.UserHabit;
import org.system.mapper.user.UserHabitMapper;
import org.system.service.iface.user.IUserHabitService;

@Service
public class UserHabitService implements IUserHabitService {
	@Resource
	private UserHabitMapper mapper;

	@Override
	public List<Map<String, Object>> getUserHabitByFild(UserHabit userHabit) {
		return mapper.checkExistsMany(userHabit);
	}

	@Override
	public int insertUserHabit(UserHabit userHabit) {
		return mapper.insert(userHabit);
	}

	@Override
	public List<Map<String, Object>> getUserHabitList(UserHabit userHabit) {
		return mapper.queryAll(userHabit);
	}

	@Override
	public int deleteUserHabit(UserHabit userHabit) {
		return mapper.delete(userHabit);
	}
}

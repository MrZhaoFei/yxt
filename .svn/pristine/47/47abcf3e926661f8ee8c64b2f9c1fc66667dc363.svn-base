<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.contract.ContractHistoryMapper" >
 
  <delete id="delete" parameterType="org.system.entity.contract.ContractHistory" >
    delete from contract_history
    where id = #{id,jdbcType=INTEGER}
      and user_id = #{userId,jdbcType=INTEGER}
      and doctor_group_id = #{doctorGroupId,jdbcType=INTEGER}
      and create_date = #{createDate,jdbcType=TIMESTAMP}
  </delete>
  <insert id="insert" parameterType="org.system.entity.contract.ContractHistory" >
    insert into contract_history (user_id, doctor_group_id, 
      create_date, creator_id, state, 
      source_id, first_send_sms_id, contract_number, 
      contract_source, user_service_pack_id, 
      update_time)
    values (#{userId,jdbcType=INTEGER}, #{doctorGroupId,jdbcType=INTEGER}, 
      #{createDate,jdbcType=TIMESTAMP}, #{creatorId,jdbcType=INTEGER}, #{state,jdbcType=INTEGER}, 
      #{sourceId,jdbcType=INTEGER}, #{firstSendSmsId,jdbcType=INTEGER}, #{contractNumber,jdbcType=VARCHAR}, 
      #{contractSource,jdbcType=INTEGER}, #{userServicePackId,jdbcType=INTEGER},
      #{updateTime,jdbcType=TIMESTAMP})
  </insert>
  <update id="update" parameterType="org.system.entity.contract.ContractHistory" >
    update contract_history
    set creator_id = #{creatorId,jdbcType=INTEGER},
      state = #{state,jdbcType=INTEGER},
      source_id = #{sourceId,jdbcType=INTEGER},
      first_send_sms_id = #{firstSendSmsId,jdbcType=INTEGER},
      contract_number = #{contractNumber,jdbcType=VARCHAR},
      contract_source = #{contractSource,jdbcType=INTEGER},
      user_service_pack_id = #{userServicePackId,jdbcType=INTEGER},
      update_time = #{updateTime,jdbcType=TIMESTAMP}
    where id = #{id,jdbcType=INTEGER}
      and user_id = #{userId,jdbcType=INTEGER}
      and doctor_group_id = #{doctorGroupId,jdbcType=INTEGER}
      and create_date = #{createDate,jdbcType=TIMESTAMP}
  </update>
  <select id="queryOne" resultType="map" parameterType="org.system.entity.contract.ContractHistory" >
    select id, user_id, doctor_group_id, create_date, creator_id, state, source_id, first_send_sms_id, 
    contract_number, contract_source, user_service_pack_id,  update_time
    from contract_history
    where id = #{id,jdbcType=INTEGER}
      and user_id = #{userId,jdbcType=INTEGER}
      and doctor_group_id = #{doctorGroupId,jdbcType=INTEGER}
      and create_date = #{createDate,jdbcType=TIMESTAMP}
  </select>
  <select id="queryAll" resultType="map"  parameterType="org.system.entity.contract.ContractHistory">
    select id, user_id, doctor_group_id, create_date, creator_id, state, source_id, first_send_sms_id, 
    contract_number, contract_source, user_service_pack_id, update_time
    from contract_history
  </select>
</mapper>
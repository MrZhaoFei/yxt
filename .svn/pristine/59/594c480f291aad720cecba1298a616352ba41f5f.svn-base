package org.system.service.iface.doctor;

import java.util.List;
import java.util.Map;

import org.system.entity.doctor.Title;

public interface ITitleService {
	
	/**
	 * 新增医生职称
	 * @param title
	 * @return int
	 */
    public int insertTitle(Title title);
	
    /**
	 * 修改医生职称
	 * @param title
	 * @return int
	 */
	public int updateTitle(Title title);
	
	/**
	 * 删除医生职称
	 * @param title
	 * @return int
	 */
	public int deleteTitle(Title title);
	
	/**
	 * 医生职称列表
	 * @param title
	 * @return List<Map<String, Object>>
	 */
	public List<Map<String, Object>> getTitleList(Title title);
	
	/**
	 * 医生职称详情
	 * @param title
	 * @return Map<String, Object>
	 */
	public Map<String, Object> getTitleDetail(Title title);
	
	/**
	 * 检查医生职称唯一性
	 * @param title
	 * @return Map<String, Object>
	 */
	public Map<String, Object> getTitleByFiled(Title title);

}

package org.system.service.impl.service;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.PackageService;
import org.system.mapper.service.PackageServiceMapper;
import org.system.service.iface.service.IPackageServiceService;

@Service
public class PackageServiceService implements IPackageServiceService{
     
	@Resource
	private PackageServiceMapper packageServiceMapper;
	
	@Override
	public int insertPackageService(PackageService packageService) {
		return packageServiceMapper.insert(packageService);
	}

	@Override
	public List<Map<String, Object>> getPackageServiceList(PackageService packageService) {
		return packageServiceMapper.queryAll(packageService);
	}

	@Override
	public Map<String, Object> getPackageServiceDetail(PackageService packageService) {
		return packageServiceMapper.queryOne(packageService);
	}

	@Override
	public Map<String, Object> getPackageServiceByFiled(PackageService packageService) {
		return packageServiceMapper.checkExists(packageService);
	}

	@Override
	public int deletePackageService(PackageService packageService) {
		return packageServiceMapper.delete(packageService);
	}

	@Override
	public int updatePackageService(PackageService packageService) {
		return packageServiceMapper.update(packageService);
	}

	@Override
	public List<Map<String, Object>> getPackageServiceByConditionID(PackageService packageService) {
		return packageServiceMapper.queryMany(packageService);
	}

	
}

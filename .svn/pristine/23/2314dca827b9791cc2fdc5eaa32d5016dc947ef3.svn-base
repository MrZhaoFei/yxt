package org.system.entity.sys;

import java.util.Date;
import org.core.entity.BaseEntity;
import org.hibernate.validator.constraints.NotBlank;
import org.utils.spring.Groups;

public class SysUserFeedback extends BaseEntity {
	private Long id;

	private Integer userId;

	private Date createDate;

	@NotBlank(message = "{sysUserFeedback.content.notblank.valid}", groups = { Groups.Insert.class })
	private String content;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}
}
package org.system.entity.contract;

import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.hibernate.validator.constraints.NotBlank;
import org.springframework.format.annotation.DateTimeFormat;
import org.system.entity.service.UserService;
import org.system.entity.user.User;
import org.system.entity.user.UserDetail;
import org.utils.spring.Groups;

public class Contract extends BaseEntity {

	public interface offlineContract {
	};

	public interface updateContractByRenew {
	};

	private Integer id;

	@NotNull(message = "{user.id.notnull.valid}")
	private Integer userId;

	private String contractNumber;

	@NotBlank(message = "{idcard.notblank.valid}", groups = { Groups.Insert.class })
	private String idcard;

	@NotNull(message = "{doctorGroup.id.notnull.valid}", groups = { Groups.Insert.class, Groups.Select.class,
			offlineContract.class, Groups.Other.class })
	private Integer doctorGroupId;

	@NotNull(message = "{doctor.id.notnull.valid}", groups = { offlineContract.class, Groups.Update.class })
	private Integer creatorId;

	private Integer state;

	private Date createDate;

	@NotNull(message = "{source.id.notnull.valid}", groups = { Groups.Insert.class })
	private Integer sourceId;

	private Integer contractSource;

	private User user;

	private UserDetail userDetail;
	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date startDate;
	@DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
	private Date endDate;

	private Integer userServicePackId;

	@NotNull(message = "{servicePack.id.notnull.valid}", groups = { updateContractByRenew.class })
	private Integer servicePackId;

	private Integer beforeState;

	private Date updateTime;

	private UserService userService;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getUserId() {
		return userId;
	}

	public void setUserId(Integer userId) {
		this.userId = userId;
	}

	public String getContractNumber() {
		return contractNumber;
	}

	public void setContractNumber(String contractNumber) {
		this.contractNumber = contractNumber;
	}

	public String getIdcard() {
		return idcard;
	}

	public void setIdcard(String idcard) {
		this.idcard = idcard;
	}

	public Integer getDoctorGroupId() {
		return doctorGroupId;
	}

	public void setDoctorGroupId(Integer doctorGroupId) {
		this.doctorGroupId = doctorGroupId;
	}

	public Integer getCreatorId() {
		return creatorId;
	}

	public void setCreatorId(Integer creatorId) {
		this.creatorId = creatorId;
	}

	public Integer getState() {
		return state;
	}

	public void setState(Integer state) {
		this.state = state;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public Integer getSourceId() {
		return sourceId;
	}

	public void setSourceId(Integer sourceId) {
		this.sourceId = sourceId;
	}

	public Integer getContractSource() {
		return contractSource;
	}

	public void setContractSource(Integer contractSource) {
		this.contractSource = contractSource;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public UserDetail getUserDetail() {
		return userDetail;
	}

	public void setUserDetail(UserDetail userDetail) {
		this.userDetail = userDetail;
	}

	public Date getStartDate() {
		return startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public Integer getUserServicePackId() {
		return userServicePackId;
	}

	public void setUserServicePackId(Integer userServicePackId) {
		this.userServicePackId = userServicePackId;
	}

	public Integer getServicePackId() {
		return servicePackId;
	}

	public void setServicePackId(Integer servicePackId) {
		this.servicePackId = servicePackId;
	}

	public Integer getBeforeState() {
		return beforeState;
	}

	public void setBeforeState(Integer beforeState) {
		this.beforeState = beforeState;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public UserService getUserService() {
		return userService;
	}

	public void setUserService(UserService userService) {
		this.userService = userService;
	}

	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class, Groups.Other.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class, Groups.Other.class })
	public Integer getRows() {
		return super.getRows();
	}
}
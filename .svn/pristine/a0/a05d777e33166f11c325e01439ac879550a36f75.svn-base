package org.system.service.impl.user.healthtest;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthtest.SelfTestTitle;
import org.system.mapper.user.healthtest.SelfTestTitleMapper;
import org.system.service.iface.user.healthtest.ISelfTestTitleService;

@Service
public class SelfTestTitleService implements ISelfTestTitleService {
	@Resource
	private SelfTestTitleMapper mapper;

	@Override
	public Map<String, Object> getSelfTestTitleByFild(SelfTestTitle selfTestTitle) {
		return mapper.checkExists(selfTestTitle);
	}

	@Override
	public int insertSelfTestTitle(SelfTestTitle selfTestTitle) {
		return mapper.insert(selfTestTitle);
	}

	@Override
	public int updateSelfTestTitle(SelfTestTitle selfTestTitle) {
		return mapper.update(selfTestTitle);
	}

	@Override
	public List<Map<String, Object>> getSelfTestTitleList(SelfTestTitle selfTestTitle) {
		return mapper.queryAll(selfTestTitle);
	}

	@Override
	public Map<String, Object> getSelfTestTitleDetail(SelfTestTitle selfTestTitle) {
		return mapper.queryOne(selfTestTitle);
	}
}

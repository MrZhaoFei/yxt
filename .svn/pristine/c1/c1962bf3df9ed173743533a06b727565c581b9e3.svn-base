package org.system.entity.doctor;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.utils.spring.Groups;

public class DoctorGroupRely extends BaseEntity {
    private Integer id;

	@NotNull(message = "{doctorGroup.id.notnull.valid}", groups = { Groups.Insert.class,Groups.Select.class })
    private Integer doctorGroupId;

	@NotNull(message = "{doctor.id.notnull.valid}", groups = { Groups.Insert.class })
    private Integer zcDoctorGroupId;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Integer getDoctorGroupId() {
        return doctorGroupId;
    }

    public void setDoctorGroupId(Integer doctorGroupId) {
        this.doctorGroupId = doctorGroupId;
    }

    public Integer getZcDoctorGroupId() {
        return zcDoctorGroupId;
    }

    public void setZcDoctorGroupId(Integer zcDoctorGroupId) {
        this.zcDoctorGroupId = zcDoctorGroupId;
    }
    
	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class })
	public Integer getRows() {
		return super.getRows();
	}
}
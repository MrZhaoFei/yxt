package org.system.service.impl.content;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.content.ContentType;
import org.system.mapper.content.ContentTypeMapper;
import org.system.service.iface.content.IContentTypeService;
@Service
public class ContentTypeService implements IContentTypeService{

	@Resource
	private ContentTypeMapper mapper;
	
	@Override
	public List<Map<String, Object>> getContentTypeList(ContentType contentType) {
		return mapper.queryAll(contentType);
	}

}

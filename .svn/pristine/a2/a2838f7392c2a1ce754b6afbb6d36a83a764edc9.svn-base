package org.system.service.impl.user.healthinfo;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthinfo.OperateHistory;
import org.system.mapper.user.healthinfo.OperateHistoryMapper;
import org.system.service.iface.user.healthinfo.IOperateHistoryService;
@Service
public class OperateHistoryService implements IOperateHistoryService{

	@Resource
	private OperateHistoryMapper mapper;

	@Override
	public int insertOperateHistory(OperateHistory operateHistory) {
		return mapper.insert(operateHistory);
	}

	@Override
	public int updateOperateHistory(OperateHistory operateHistory) {
		return mapper.update(operateHistory);
	}

	@Override
	public Map<String, Object> getOperateHistoryDetail(
			OperateHistory operateHistory) {
		return mapper.queryOne(operateHistory);
	}

	@Override
	public List<Map<String, Object>> getOperateHistorys(
			OperateHistory operateHistory) {
		return mapper.queryAll(operateHistory);
	}

	@Override
	public int deleteOperateHistory(OperateHistory operateHistory) {
		return mapper.delete(operateHistory);
	}


	
	
	

}

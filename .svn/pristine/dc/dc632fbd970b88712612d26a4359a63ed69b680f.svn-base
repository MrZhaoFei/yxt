package org.system.service.impl.order;

import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.order.PayType;
import org.system.mapper.order.PackagePayTypeMapper;
import org.system.service.iface.order.IPackagePayTypeService;
@Service
public class PackagePayTypeService implements IPackagePayTypeService{

	@Resource
	private PackagePayTypeMapper mapper;
	
	@Override
	public Map<String, Object> getPayTypeDetail(PayType payType) {
		return mapper.queryOne(payType);
	}
}

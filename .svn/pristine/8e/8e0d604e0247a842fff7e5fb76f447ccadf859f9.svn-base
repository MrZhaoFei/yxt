package org.system.service.impl.service.transfer;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.transfer.ImageConsultationRecordsInfo;
import org.system.mapper.service.transfer.ImageConsultationRecordsInfoMapper;
import org.system.service.iface.service.transfer.IImageConsultationRecordsInfoService;

@Service
public class ImageConsultationRecordsInfoService implements IImageConsultationRecordsInfoService{

	@Resource
	private ImageConsultationRecordsInfoMapper mapper;
	
	@Override
	public int insertImageConsultationRecordsInfo(ImageConsultationRecordsInfo imageConsultationRecordsInfo) {
		return mapper.insert(imageConsultationRecordsInfo);
	}

	@Override
	public List<Map<String, Object>> getImageConsultationRecordsInfoList(
			ImageConsultationRecordsInfo imageConsultationRecordsInfo) {
		return mapper.queryAll(imageConsultationRecordsInfo);
	}

	@Override
	public Map<String, Object> getImageConsultationRecordsInfoDetail(
			ImageConsultationRecordsInfo imageConsultationRecordsInfo) {
		return mapper.queryOne(imageConsultationRecordsInfo);
	}
}

package org.system.service.impl.user.healthinfo;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.user.healthinfo.DiseaseHistory;
import org.system.mapper.user.healthinfo.DiseaseHistoryMapper;
import org.system.service.iface.user.healthinfo.IDiseaseHistoryService;

@Service
public class DiseaseHistoryService implements IDiseaseHistoryService{

	@Resource
	private DiseaseHistoryMapper mapper;
	
	
	@Override
	public int insertDiseaseHistory(DiseaseHistory diseaseHistory) {
		return mapper.insert(diseaseHistory);
	}

	@Override
	public int updateDiseaseHistory(DiseaseHistory diseaseHistory) {
		return mapper.update(diseaseHistory);
	}

	@Override
	public Map<String, Object> getDiseaseHistoryDetail(DiseaseHistory diseaseHistory) {
		return mapper.queryOne(diseaseHistory);
	}

	@Override
	public List<Map<String, Object>> getDiseaseHistorys(DiseaseHistory diseaseHistory) {
		return mapper.queryAll(diseaseHistory);
	}

	@Override
	public int deleteDiseaseHistory(DiseaseHistory diseaseHistory) {
		return mapper.delete(diseaseHistory);
	}

}

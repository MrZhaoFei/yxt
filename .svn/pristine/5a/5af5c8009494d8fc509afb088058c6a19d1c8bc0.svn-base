package org.system.service.impl.doctor;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.doctor.DoctorDoctorGroup;
import org.system.mapper.doctor.DoctorDoctorGroupMapper;
import org.system.service.iface.doctor.IDoctorDoctorGroupService;
@Service
public class DoctorDoctorGroupService implements IDoctorDoctorGroupService{
    @Resource
    private DoctorDoctorGroupMapper doctorDoctorGroupMapper;
    
	@Override
	public int insertDoctorDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.insert(doctorDoctorGroup);
	}

	@Override
	public int deleteDoctorDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.delete(doctorDoctorGroup);
	}

	@Override
	public Map<String, Object> getDoctorDoctorGroupByFiled(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.checkExists(doctorDoctorGroup);
	}

	@Override
	public int updateDoctorDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.update(doctorDoctorGroup);
	}

	@Override
	public List<Map<String, Object>> getDoctorDoctorGroups(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.queryAll(doctorDoctorGroup);
	}

	@Override
	public int getOneTeamer(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.queryOneTeamer(doctorDoctorGroup);
	}

	@Override
	public int getOneDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.queryOneDoctorGroup(doctorDoctorGroup);
	}

	@Override
	public int getOneDoctorExistsGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return doctorDoctorGroupMapper.queryOneDoctorExistsGroup(doctorDoctorGroup);
	}

	

}

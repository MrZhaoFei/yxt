package org.system.service.impl.service.transfer;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.service.transfer.TransTreatDetaily;
import org.system.mapper.service.transfer.TransTreatDetailyMapper;
import org.system.service.iface.service.transfer.ITransTreatDetailyService;

@Service
public class TransTreatDetailyService implements ITransTreatDetailyService {
	@Resource
	private TransTreatDetailyMapper mapper;

	@Override
	public Map<String, Object> getTransTreatDetailyByFild(TransTreatDetaily transTreatDetaily) {
		return mapper.checkExists(transTreatDetaily);
	}

	@Override
	public int insertTransTreatDetaily(TransTreatDetaily transTreatDetaily) {
		return mapper.insert(transTreatDetaily);
	}

	@Override
	public int updateTransTreatDetaily(TransTreatDetaily transTreatDetaily) {
		return mapper.update(transTreatDetaily);
	}

	@Override
	public List<Map<String, Object>> getTransTreatDetailyList(TransTreatDetaily transTreatDetaily) {
		return mapper.queryAll(transTreatDetaily);
	}

	@Override
	public Map<String, Object> getTransTreatDetailyDetail(TransTreatDetaily transTreatDetaily) {
		return mapper.queryOne(transTreatDetaily);
	}

	@Override
	public List<Map<String, Object>> getTransTreatDetailyDoctorByUserList(TransTreatDetaily transTreatDetaily) {
		return mapper.getTransTreatDetailyDoctorByUserList(transTreatDetaily);
	}

	@Override
	public int insertTransTreatDetailyFiles(TransTreatDetaily transTreatDetaily) {
		return mapper.insertTransTreatDetailyFiles(transTreatDetaily);
	}
}

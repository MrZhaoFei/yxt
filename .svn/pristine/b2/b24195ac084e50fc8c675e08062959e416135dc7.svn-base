<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.doctor.DoctorDoctorGroupRecordMapper" >
<!-- 新增 -->
<insert id="insert" parameterType="org.system.entity.doctor.DoctorDoctorGroupRecord" useGeneratedKeys="true" keyColumn="id">
insert into doctor_doctor_group_record (doctor_group_id,doctor_id,create_date,type) values (#{doctorGroupId},#{doctorId},#{createDate},#{type},#{remark},#{hospitalId})
</insert>

<update id="update" parameterType="org.system.entity.doctor.DoctorDoctorGroupRecord" >
    update doctor_doctor_group_record 
 <set>		
		<if test="createDate !=null and createDate!=''">
			create_date=#{createDate},
		</if>
		<if test="type !=null and type!=''">
			type=#{type},
		</if>
	</set>
	where doctor_group_id=#{doctorGroupId}
</update>

</mapper>
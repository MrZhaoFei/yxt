package org.system.entity.service.resp;

import java.util.Date;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.utils.spring.Groups;

public class TransRecordsInfo extends BaseEntity {
	private Integer id;

	@NotNull(message = "{serviceRespTask.id.notnull.valid}",groups={Groups.Insert.class})
	private Integer serviceRespTaskId;

	private Integer srcDoctorId;

	@NotNull(message = "{doctor.id.notnull.valid}",groups={Groups.Insert.class})
	private Integer descDoctorId;

	private Date transDate;

	private String transDesc;
	

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Integer getServiceRespTaskId() {
		return serviceRespTaskId;
	}

	public void setServiceRespTaskId(Integer serviceRespTaskId) {
		this.serviceRespTaskId = serviceRespTaskId;
	}

	public Integer getSrcDoctorId() {
		return srcDoctorId;
	}

	public void setSrcDoctorId(Integer srcDoctorId) {
		this.srcDoctorId = srcDoctorId;
	}

	public Integer getDescDoctorId() {
		return descDoctorId;
	}

	public void setDescDoctorId(Integer descDoctorId) {
		this.descDoctorId = descDoctorId;
	}

	public Date getTransDate() {
		return transDate;
	}

	public void setTransDate(Date transDate) {
		this.transDate = transDate;
	}

	public String getTransDesc() {
		return transDesc;
	}

	public void setTransDesc(String transDesc) {
		this.transDesc = transDesc;
	}
}
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.system.mapper.user.healthtest.SelfTestTitleMapper" >
	<!-- 新增 -->
	<insert id="insert" parameterType="org.system.entity.user.healthtest.SelfTestTitle">
		
	</insert>
	
	<!-- 删除 -->
	<delete id="delete" parameterType="org.system.entity.user.healthtest.SelfTestTitle">
		
	</delete>
	
	<!-- 修改 -->
	<update id="update" parameterType="org.system.entity.user.healthtest.SelfTestTitle">
		
	</update>
	
	<!-- 查询列表 -->
	<select id="queryAll" parameterType="org.system.entity.user.healthtest.SelfTestTitle" resultType="map">
		select id,name,`desc`,pic_url picUrl from self_test_title
		<where>
			<if test="name !=null and name !=''">
				name like CONCAT(CONCAT('%', #{name}), '%')
			</if>
		</where>
	</select>
	<!-- 根据主键查询单个 -->
	<select id="queryOne" parameterType="org.system.entity.user.healthtest.SelfTestTitle" resultMap="questionMap">
		select id,name,`desc`,pic_url picUrl from self_test_title where id=#{id}
	</select>
	<resultMap type="map" id="questionMap">
		 <result property="id" column="id" />
		<collection property="question" column="id" javaType="list" select="getQuestion"/>
		<collection property="rule" column="id" javaType="list" select="getResult"/>
	</resultMap>
	<select id="getQuestion" parameterType="map" resultMap="answerMap">
		select id,question,view_order viewOrder from self_test_question where title_id=#{id}
	</select>
	<resultMap type="map" id="answerMap">
		 <result property="id" column="id" />
		<collection property="answer" column="id" javaType="list" select="getAnswer"/>
	</resultMap>
	<select id="getAnswer" parameterType="map" resultType="map">
		select id,answer,score from self_test_answer where question_id=#{id}
	</select>
	<select id="getResult" parameterType="map" resultType="map">
		select id,title,score_start scoreStart,`desc`,score_end scoreEnd from self_test_result where title_id=#{id}
	</select>
	
	<!-- 根据约束键查询数据是否存在 -->
	<select id="checkExists" parameterType="org.system.entity.user.healthtest.SelfTestTitle" resultType="map">
	
	</select>
</mapper>

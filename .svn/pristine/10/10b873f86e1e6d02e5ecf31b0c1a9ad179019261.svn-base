package org.system.service.impl.chat;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.chat.WeixinRecordsHistoryInfo;
import org.system.mapper.chat.WeixinRecordsHistoryInfoMapper;
import org.system.service.iface.chat.IWeixinRecordsHistoryInfoService;

@Service
public class WeixinRecordsHistoryInfoService implements IWeixinRecordsHistoryInfoService {
	@Resource
	private WeixinRecordsHistoryInfoMapper mapper;

	@Override
	public List<Map<String, Object>> getWeixinRecordsHistoryInfoList(WeixinRecordsHistoryInfo weixinRecordsHistoryInfo) {
		return mapper.queryAll(weixinRecordsHistoryInfo);
	}

}

package org.system.service.impl.hospiDepartDoctorRelation;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.relation.HospiDepartDoctorRelation;
import org.system.mapper.relation.HospiDepartDoctorRelationMapper;
import org.system.service.iface.HospiDepartDoctorRelation.IHospiDepartDoctorRelationService;

@Service
public class HospiDepartDoctorRelationService implements IHospiDepartDoctorRelationService{
    @Resource
    private HospiDepartDoctorRelationMapper hospiDepartDoctorRelationMapper;

	@Override
	public int insertHospiDepartDoctorRelation(HospiDepartDoctorRelation hospiDepartDoctorRelation) {
		return hospiDepartDoctorRelationMapper.insert(hospiDepartDoctorRelation);
	}

	@Override
	public int updateHospiDepartDoctorRelation(HospiDepartDoctorRelation hospiDepartDoctorRelation) {
		return hospiDepartDoctorRelationMapper.update(hospiDepartDoctorRelation);
	}

	@Override
	public Map<String, Object> getHospiDepartDoctorRelationByFiled(
			HospiDepartDoctorRelation hospiDepartDoctorRelation) {
		return hospiDepartDoctorRelationMapper.checkExists(hospiDepartDoctorRelation);
	}

	@Override
	public List<Map<String, Object>> getHospDept(HospiDepartDoctorRelation hospiDepartDoctorRelation) {
		return hospiDepartDoctorRelationMapper.queryHospDept(hospiDepartDoctorRelation);
	}

	@Override
	public int deleteHospiDepartDoctorRelation(HospiDepartDoctorRelation hospiDepartDoctorRelation) {
		return hospiDepartDoctorRelationMapper.delete(hospiDepartDoctorRelation);
	}
}

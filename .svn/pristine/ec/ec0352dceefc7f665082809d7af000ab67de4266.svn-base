package org.system.service.impl.doctor;

import java.util.List;
import java.util.Map;

import javax.annotation.Resource;

import org.springframework.stereotype.Service;
import org.system.entity.doctor.DoctorDoctorGroup;
import org.system.mapper.doctor.DoctorDoctorGroupMapper;
import org.system.service.iface.doctor.IDoctorDoctorGroupService;
@Service
public class DoctorDoctorGroupService implements IDoctorDoctorGroupService{
    @Resource
    private DoctorDoctorGroupMapper mapper;
    
	@Override
	public int insertDoctorDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.insert(doctorDoctorGroup);
	}

	@Override
	public int deleteDoctorDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.delete(doctorDoctorGroup);
	}

	@Override
	public Map<String, Object> getDoctorDoctorGroupByFiled(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.checkExists(doctorDoctorGroup);
	}

	@Override
	public int updateDoctorDoctorGroup(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.update(doctorDoctorGroup);
	}

	@Override
	public List<Map<String, Object>> getDoctorDoctorGroups(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.queryAll(doctorDoctorGroup);
	}

	@Override
	public Map<String, Object> getDoctorDoctorGroupDetail(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.queryOne(doctorDoctorGroup);
	}

	@Override
	public Map<String, Object> getTransTreatDetaily(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.getTransTreatDetaily(doctorDoctorGroup);
	}

	@Override
	public Map<String, Object> getImageConsultationDetaily(DoctorDoctorGroup doctorDoctorGroup) {
		return mapper.getImageConsultationDetaily(doctorDoctorGroup);
	}

	@Override
	public Map<String, Object> getServiceRespTaskDetaily(DoctorDoctorGroup doctorDoctorGroup) {
		return null;
	}

	

}

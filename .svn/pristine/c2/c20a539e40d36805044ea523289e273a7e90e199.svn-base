package org.system.entity.address;

import javax.validation.constraints.NotNull;

import org.core.entity.BaseEntity;
import org.hibernate.validator.constraints.NotBlank;
import org.utils.spring.Groups;

public class Address extends BaseEntity {
	private Integer id;
	private Integer hospitalId;

	private String pid;

	private String codeValue;

	@NotBlank(message = "{address.name.notblank.valid}", groups = { Groups.Insert.class })
	private String name;
	@NotNull(message = "{address.raidus.notnull.valid}", groups = { Groups.Other.class })
	private Integer raidus;
	@NotNull(message = "{address.longitude.notnull.valid}", groups = { Groups.Other.class })
	private Float longitude;
	@NotNull(message = "{address.latitude.notnull.valid}", groups = { Groups.Other.class })
	private Float latitude;

	private Boolean isParent;

	private String remark;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getPid() {
		return pid;
	}

	public void setPid(String pid) {
		this.pid = pid;
	}

	public String getCodeValue() {
		return codeValue;
	}

	public void setCodeValue(String codeValue) {
		this.codeValue = codeValue;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Boolean getIsParent() {
		return isParent;
	}

	public void setIsParent(Boolean isParent) {
		this.isParent = isParent;
	}

	public Float getLongitude() {
		return longitude;
	}

	public void setLongitude(Float longitude) {
		this.longitude = longitude;
	}

	public Float getLatitude() {
		return latitude;
	}

	public void setLatitude(Float latitude) {
		this.latitude = latitude;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public Integer getRaidus() {
		return raidus;
	}

	public void setRaidus(Integer raidus) {
		this.raidus = raidus;
	}

	public Integer getHospitalId() {
		return hospitalId;
	}

	public void setHospitalId(Integer hospitalId) {
		this.hospitalId = hospitalId;
	}

	@Override
	@NotNull(message = "{page.empty}", groups = { Groups.Select.class })
	public Integer getPage() {
		return super.getPage();
	}

	@Override
	@NotNull(message = "{rows.empty}", groups = { Groups.Select.class })
	public Integer getRows() {
		return super.getRows();
	}
}